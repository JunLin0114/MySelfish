echo "kbpress 3 2 0" > ${DEV}
sleep ${TIME}
echo "kbpress 2 7 1" > ${DEV}
echo "kbpress 2 7 0" > ${DEV}
sleep ${TIME}
echo "kbpress 4 4 1" > ${DEV}
echo "kbpress 4 4 0" > ${DEV}
sleep ${TIME}
