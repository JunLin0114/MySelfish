echo "kbpress 8 6 0" > ${DEV}
sleep ${TIME}
echo "kbpress 8 6 1" > ${DEV}
echo "kbpress 8 6 0" > ${DEV}
sleep ${TIME}
echo "kbpress 11 4 1" > ${DEV}
echo "kbpress 11 4 0" > ${DEV}
sleep 2
echo "kbpress 3 7 1" > ${DEV}
echo "kbpress 3 7 0" > ${DEV}
sleep ${TIME}
